{
  "name": "cat-energy",
  "version": "14.0.0",
  "private": true,
  "description": "Личный проект «Кэт энерджи» от HTML Academy",
  "devDependencies": {
    "@htmlacademy/editorconfig-cli": "1.x",
    "autoprefixer": "8.3.x",
    "browser-sync": "2.23.x",
    "del": "^3.0.0",
    "gulp": "3.9.x",
    "gulp-bem-css": "^1.1.3",
    "gulp-csso": "^3.0.1",
    "gulp-imagemin": "^4.1.0",
    "gulp-plumber": "1.2.x",
    "gulp-postcss": "7.0.x",
    "gulp-posthtml": "^3.0.4",
    "gulp-rename": "^1.2.3",
    "gulp-sass": "4.0.x",
    "gulp-svgstore": "^6.1.1",
    "gulp-uglify": "^3.0.0",
    "gulp-webp": "^3.0.0",
    "picturefill": "^3.0.2",
    "postcss-normalize": "^4.0.0",
    "posthtml-include": "^1.1.0",
    "pump": "^3.0.0",
    "run-sequence": "^2.2.1",
    "stylelint": "9.2.x",
    "stylelint-config-htmlacademy": "0.1.x"
  },
  "scripts": {
    "editorconfig": "editorconfig-cli",
    "stylelint": "stylelint \"source/sass/**/*.scss\" --syntax scss",
    "test": "npm run editorconfig && npm run stylelint",
    "build": "gulp build",
    "start": "npm run build && gulp serve",
    "picturefill": "gulp picturefill",
    "images": "gulp imagemin",
    "webp": "gulp webp",
    "sprite": "gulp svgstore",
    "html": "gulp posthtml",
    "include": "gulp include",
    "copy": "gulp copy",
    "clean": "gulp del"
  },
  "browserslist": [
    "last 2 versions",
    "IE 11",
    "Firefox ESR"
  ],
  "editorconfig-cli": [
    "*.json",
    "*.js",
    "source/*.html",
    "source/js/**/*.js",
    "source/img/**/*.svg",
    "source/sass/**/*.{sass,scss}"
  ],
  "engines": {
    "node": "8.11"
  }
}
